<div>
      {{#each user}}
      <div style="border: 1px solid black">
        <p>`About me: {{this.userName}} from {{this.city}}` </p>
        <img src={{this.photo}} alt={{this.userName}} height="250px" width="350px">
        <a href="#">back to the item</a>
        <p>{{this.description}}</p>
        <p>Only {{this.itemsRemaining}} left in stock</p>
      </div>
    </div>
     <div>
         <p>Goods from {{this.userName}}</p>
     {{/each}}

      {{#each products}}
      <div style="border: 1px solid black">
        <p>{{this.name}}</p>
        <p>{{this.photo}}</p>

        {{!-- <p>{{this.category}}</p>
        <p>{{this.description}}</p>
        <img style="height: 200px;" src="{{this.photo}}">
        <p>${{this.price}}</p> --}}
        {{!-- <p>Only {{this.itemsRemaining}} left in stock</p> --}}
      </div>
      {{/each}}
</div>
